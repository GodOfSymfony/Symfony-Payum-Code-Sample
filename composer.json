{
    "name": "ecommerce121/munireg",
    "license": "proprietary",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Ecommerce121\\UtilBundle\\": "ecommerce121/util-bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.121ecommerce.com/"
        }
    ],
    "require": {
        "php": ">=7.3",
        "box/spout": "^2.7",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "friendsofsymfony/ckeditor-bundle": "^2.0.0",
        "friendsofsymfony/jsrouting-bundle": "2.8.0",
        "jms/serializer-bundle": "^3.2.0",
        "knplabs/knp-snappy-bundle": "^1.8",
        "payum/authorize-net-aim": "^1.5",
        "payum/payum-bundle": "^2.3",
        "php-http/guzzle6-adapter": "^1.1",
        "sensio/framework-extra-bundle": "^6.2.5",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/css-selector": "5.4.*",
        "symfony/dotenv": "^5.4",
        "symfony/flex": "^1.17",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-kernel": "5.4.*",
        "symfony/maker-bundle": "^1.27.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/security-bundle": "5.4.*",
        "symfony/swiftmailer-bundle": "^3.5.3",
        "symfony/templating": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.0",
        "symfony/webpack-encore-bundle": "^1.13",
        "twig/extra-bundle": "^3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.2",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "2.7.0",
        "symfony/maker-bundle": "^1.36",
        "symfony/phpunit-bridge": "^4.4"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
